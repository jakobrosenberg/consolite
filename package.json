{
  "name": "consolite",
  "version": "0.3.1",
  "description": "Light wrapper for console",
  "main": "./cjs/index.js",
  "module": "./esm/index.mjs",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./esm/index.mjs"
    }
  },
  "types": "./typings/index.d.ts",
  "scripts": {
    "test": "probs test",
    "build": "run-p build:*",
    "build:ts": "npx tsc",
    "build:cjs": "babel esm -d cjs --presets @babel/preset-env -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakobrosenberg/consolite.git"
  },
  "keywords": [
    "log",
    "logging",
    "debug",
    "console",
    "print"
  ],
  "author": "jakobrosenberg@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jakobrosenberg/consolite/issues"
  },
  "homepage": "https://github.com/jakobrosenberg/consolite#readme",
  "prettier": {
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "semi": false,
    "useTabs": false,
    "printWidth": 90
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.6.0-dev.20211104"
  },
  "dependencies": {
    "probs": "^1.0.0-3"
  }
}
